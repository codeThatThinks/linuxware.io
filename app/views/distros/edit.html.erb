<div class="page-header">
	<h1>Edit <%= @distro.name %></h1>
</div>
<div class="row">
	<div class="col-xs-12">
		<%= form_for @distro, url: @distro do |f| %>
			<div class="form-group">
				<%= f.label :name %>
				<%=	f.text_field :name, class: "form-control" %>
			</div>
			<%= f.submit class: "btn btn-default" %>
		<% end %>
	</div>
</div>
